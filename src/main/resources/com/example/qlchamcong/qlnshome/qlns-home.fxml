      <Label fx:id="homePageText" layoutX="95.0" layoutY="14.0" prefHeight="28.0" prefWidth="76.0" text="Home Page" />
      <TableView fx:id="attendanceTableView" prefWidth="600">
         <columns>
            <TableColumn text="ID" fx:id="idColumn"/>
