            <TableColumn text="Ngày" fx:id="dateColumn"/>
            <TableColumn text="Số giờ ca 1" fx:id="hoursShift1Column"/>
            <TableColumn text="Số giờ ca 2" fx:id="hoursShift2Column"/>
            <TableColumn text="Số giờ ca 3" fx:id="hoursShift3Column"/>
