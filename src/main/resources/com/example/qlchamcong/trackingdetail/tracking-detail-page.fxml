<AnchorPane prefHeight="526.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.qlchamcong.trackingdetail.TrackingDetailPageController">
