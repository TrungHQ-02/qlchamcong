            <TableColumn prefWidth="87.0" text="Nickname" />
            <TableColumn prefWidth="115.0" text="Employee" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
